pipeline {
 agent any
tools {
  maven 'My_Maven_Jenkins'
      }
stages{
  stage('Git Pull') {
       steps {
          git 'https://github.com/Akash-Softw/InsureMeProject.git'
    
            }
}
 stage('Build Package') {
       steps {
         sh 'mvn package'
    
            }
        }
stage('Public HTML Reports') {
       steps {
publishHTML([allowMissing: false, alwaysLinkToLastBuild: false, keepAll: false, reportDir: 'var/lib/jenkins/workspace/Insureme-StarAgile/target/surefire-reports', reportFiles: 'index.html', reportName: 'HTML Report', reportTitles: '', useWrapperFileDirectly: true])    
            }
        }
    }
}
